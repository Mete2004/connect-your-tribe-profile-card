{% render 'partials/head.liquid' %}

<article class="profile-card-example">

  <header class="profile-card-example-header">

    <span class="profile-badge">Web Dev Student</span>

    <h1>{{ person.name }}</h1>
    <p><em>Frontend Developer</em></p>

    <button class="profile-card-example-button">
      <!-- expand -->
      <svg width="24" height="24" viewBox="0 0 24 24">
        <path d="M4 8V6a2 2 0 0 1 2-2h2M4 16v2a2 2 0 0 0 2 2h2M16 4h2a2 2 0 0 1 2 2v2M16 20h2a2 2 0 0 0 2-2v-2"/>
      </svg>
      <!-- collapse -->
      <svg width="24" height="24" viewBox="0 0 24 24">
        <path d="M15 19v-2a2 2 0 0 1 2-2h2M15 5v2a2 2 0 0 0 2 2h2M5 15h2a2 2 0 0 1 2 2v2M5 9h2a2 2 0 0 0 2-2v-2"/>
      </svg>
    </button>

  </header>

  <img src="assets/Squad1J-Mete.jpeg" class="profile-card-example-img" alt="">

  <dl class="profile-card-example-dl">
    <dt>HTML</dt><dd><img src="https://cdn.jsdelivr.net/gh/devicons/devicon/icons/html5/html5-original.svg"></dd>
    <dt>CSS</dt><dd><img src="https://cdn.jsdelivr.net/gh/devicons/devicon/icons/css3/css3-original.svg"></dd>
    <dt>JavaScript</dt><dd><img src="https://cdn.jsdelivr.net/gh/devicons/devicon/icons/javascript/javascript-original.svg"></dd>
  </dl>

<footer class="profile-footer">
  <p class="profile-text">
    Ik bouw moderne, snelle en interactieve webinterfaces.
  </p>

  <a href="#" class="profile-cta">
    Bekijk portfolio
  </a>
</footer>


</article>

    <script src="../script.js"></script>
    <script src="../server.js"></script>

{% render 'partials/foot.liquid' %}
